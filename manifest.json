{
    "manifest_version": 3,
    "name": "Image Hasher",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus" 
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }]
  }
  